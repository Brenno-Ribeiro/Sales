<?xml version="1.0" encoding="utf-8"?>
<Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2008/01/reportdefinition"
        xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">
  <DataSources>
    <DataSource Name="DataSource1">
      <ConnectionProperties>
        <DataProvider>System.Data.DataSet</DataProvider>
        <ConnectString />
      </ConnectionProperties>
      <rd:DataSourceID>00000000-0000-0000-0000-000000000000</rd:DataSourceID>
    </DataSource>
  </DataSources>

  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="id"><DataField>id</DataField><rd:TypeName>System.Int32</rd:TypeName></Field>
        <Field Name="product"><DataField>product</DataField><rd:TypeName>System.String</rd:TypeName></Field>
        <Field Name="total_sold"><DataField>total_sold</DataField><rd:TypeName>System.Int32</rd:TypeName></Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText />
      </Query>
    </DataSet>
  </DataSets>

  <Body>
    <ReportItems>
      <Tablix Name="Tablix1">
        <TablixBody>
          <TablixColumns>
            <TablixColumn><Width>3in</Width></TablixColumn>
            <TablixColumn><Width>1.5in</Width></TablixColumn>
          </TablixColumns>
          <TablixRows>
            <TablixRow>
              <Height>0.25in</Height>
              <TablixCells>
                <TablixCell>
                  <CellContents>
                    <Textbox Name="HeaderProduct">
                      <CanGrow>true</CanGrow>
                      <Style>
                        <FontWeight>Bold</FontWeight>
                        <BackgroundColor>#D9EDF7</BackgroundColor>
                        <Border>
                          <Style>Solid</Style>
                          <Color>Black</Color>
                        </Border>
                        <TextAlign>Center</TextAlign>
                      </Style>
                      <Paragraphs>
                        <Paragraph>
                          <TextRuns>
                            <TextRun><Value>Produto</Value></TextRun>
                          </TextRuns>
                        </Paragraph>
                      </Paragraphs>
                    </Textbox>
                  </CellContents>
                </TablixCell>

                <TablixCell>
                  <CellContents>
                    <Textbox Name="HeaderTotal">
                      <CanGrow>true</CanGrow>
                      <Style>
                        <FontWeight>Bold</FontWeight>
                        <BackgroundColor>#D9EDF7</BackgroundColor>
                        <Border>
                          <Style>Solid</Style>
                          <Color>Black</Color>
                        </Border>
                        <TextAlign>Center</TextAlign>
                      </Style>
                      <Paragraphs>
                        <Paragraph>
                          <TextRuns>
                            <TextRun><Value>Total Vendido</Value></TextRun>
                          </TextRuns>
                        </Paragraph>
                      </Paragraphs>
                    </Textbox>
                  </CellContents>
                </TablixCell>
              </TablixCells>
            </TablixRow>
            <TablixRow>
              <Height>0.2in</Height>
              <TablixCells>
                <TablixCell>
                  <CellContents>
                    <Textbox Name="ProductValue">
                      <CanGrow>true</CanGrow>
                      <Style>
                        <Border>
                          <Style>Solid</Style>
                          <Color>Black</Color>
                        </Border>
                        <BackgroundColor>=IIF(RowNumber(Nothing) Mod 2 = 0, "#F9F9F9", "White")</BackgroundColor>
                        <TextAlign>Left</TextAlign>
                      </Style>
                      <Paragraphs>
                        <Paragraph>
                          <TextRuns>
                            <TextRun><Value>=Fields!product.Value</Value></TextRun>
                          </TextRuns>
                        </Paragraph>
                      </Paragraphs>
                    </Textbox>
                  </CellContents>
                </TablixCell>
                <TablixCell>
                  <CellContents>
                    <Textbox Name="TotalSoldValue">
                      <CanGrow>true</CanGrow>
                      <Style>
                        <Border>
                          <Style>Solid</Style>
                          <Color>Black</Color>
                        </Border>
                        <BackgroundColor>=IIF(RowNumber(Nothing) Mod 2 = 0, "#F9F9F9", "White")</BackgroundColor>
                        <TextAlign>Center</TextAlign>
                      </Style>
                      <Paragraphs>
                        <Paragraph>
                          <TextRuns>
                            <TextRun><Value>=Fields!total_sold.Value</Value></TextRun>
                          </TextRuns>
                        </Paragraph>
                      </Paragraphs>
                    </Textbox>
                  </CellContents>
                </TablixCell>
              </TablixCells>
            </TablixRow>
          </TablixRows>
        </TablixBody>

        <DataSetName>DataSet1</DataSetName>

        <TablixColumnHierarchy>
          <TablixMembers>
            <TablixMember />
            <TablixMember />
          </TablixMembers>
        </TablixColumnHierarchy>

        <TablixRowHierarchy>
          <TablixMembers>
            <TablixMember />
            <TablixMember>
              <Group Name="DetailGroup" />
            </TablixMember>
          </TablixMembers>
        </TablixRowHierarchy>

        <Top>0.2in</Top>
        <Left>0.2in</Left>
        <Height>0.6in</Height>
        <Width>4.5in</Width>
        <NoRowsMessage>Nenhum produto vendido encontrado.</NoRowsMessage>
      </Tablix>
    </ReportItems>
    <Height>2in</Height>
  </Body>

  <Width>6.5in</Width>
  <Page>
    <PageHeight>11in</PageHeight>
    <PageWidth>8.5in</PageWidth>
    <LeftMargin>0.5in</LeftMargin>
    <RightMargin>0.5in</RightMargin>
    <TopMargin>0.5in</TopMargin>
    <BottomMargin>0.5in</BottomMargin>
  </Page>
</Report>